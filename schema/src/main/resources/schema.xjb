<bindings version="2.0" xmlns="http://java.sun.com/xml/ns/jaxb"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
    extensionBindingPrefixes="xjc"
    schemaLocation="springer.xsd" node="/xs:schema">
	<globalBindings localScoping="toplevel" generateValueClass="false">
		<javaType name="double" xmlType="xs:double"/>
		<javaType name="long" xmlType="xs:integer"/>
		<xjc:serializable uid="2461660169443089969"/>
		<xjc:generateElementProperty>false</xjc:generateElementProperty>
		<xjc:simple />
		<javaType name="java.util.Date" xmlType="xs:dateTime"
	      parseMethod="com.springer.api.schema.common.DateAdapter.parseDate"
	      printMethod="com.springer.api.schema.common.DateAdapter.printDate"
	    />		 		
	</globalBindings>
	<nameXmlTransform>
          <typeName suffix="Type"/>
    </nameXmlTransform> 
	
