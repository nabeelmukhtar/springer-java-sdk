        </plugin>
      </plugins>
    </pluginManagement> 
  </build>
  <dependencies>
	<dependency>
	    <groupId>net.sf.kxml</groupId>
	    <artifactId>kxml2</artifactId>
	    <version>2.2.2</version>
	</dependency>
  </dependencies>  
</project>